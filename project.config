[project]
	description = A Kubernetes Framework for Apache Mesos
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Joerg (joerg84)
[access "refs/for/refs/*"]
	read = group user/Joerg (joerg84)
	push = group user/Joerg (joerg84)
	label-Code-Review = -2..+2 group user/Joerg (joerg84)
	label-Verified = -1..+1 group user/Joerg (joerg84)
	rebase = group user/Joerg (joerg84)
	removeReviewer = group user/Joerg (joerg84)
	submit = group user/Joerg (joerg84)
[access "refs/heads/*"]
	read = group user/Joerg (joerg84)
	create = group user/Joerg (joerg84)
	push = +force group user/Joerg (joerg84)
	pushMerge = group user/Joerg (joerg84)
[access "refs/tags/*"]
	push = +force group user/Joerg (joerg84)
	pushTag = group user/Joerg (joerg84)
	pushSignedTag = group user/Joerg (joerg84)
